{
  "manifest_version": 3,
  "name": "Paging Fantia Post Images with arrow keys",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["https://fantia.jp/posts/*"],
      "js": ["fantia.js"]
    },
    {
      "matches": ["https://*.fanbox.cc/posts/*"],
      "js": ["fanbox.js"]
    }
  ]
}
